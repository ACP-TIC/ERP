          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SalesByDateTableAdapter" GeneratorDataComponentClassName="SalesByDateTableAdapter" Name="SalesByDate" UserDataComponentName="SalesByDateTableAdapter">
