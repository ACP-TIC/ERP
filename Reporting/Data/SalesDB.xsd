          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SalesInDetailTableAdapter" GeneratorDataComponentClassName="SalesInDetailTableAdapter" Name="SalesInDetail" UserDataComponentName="SalesInDetailTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="cognitivogoldenConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT app_branch.name AS Branch, contacts.name AS Customer, contacts.gov_code AS Gov_Code, contacts.code AS Cust_Code, contacts.telephone AS Telephone, items.code AS Code, sales_invoice_detail.item_description AS Item, sales_invoice_detail.quantity AS Quantity, SUM(app_vat.coefficient) AS Coefficient, 
         sales_invoice_detail.unit_price AS `Unit Price`, sales_invoice_detail.unit_cost AS `Unit Cost`, sales_invoice_detail.discount AS `Unit Discount`, sales_invoice_detail.unit_price * (SUM(app_vat.coefficient) + 1) AS `Unit Price with VAT`, sales_invoice_detail.unit_price * (SUM(app_vat.coefficient) + 1) * sales_invoice_detail.quantity AS `Sub Total with VAT`, 
         sales_invoice.number AS Number, sales_invoice.trans_date AS `Date`, sales_invoice.comment AS Comment, sales_rep.name
FROM  app_branch RIGHT OUTER JOIN
         sales_invoice INNER JOIN
         sales_invoice_detail ON sales_invoice.id_sales_invoice = sales_invoice_detail.id_sales_invoice LEFT OUTER JOIN
         sales_rep ON sales_invoice.id_sales_rep = sales_rep.id_sales_rep LEFT OUTER JOIN
         contacts ON sales_invoice.contact_id_contact = contacts.id_contact ON app_branch.id_branch = sales_invoice.id_branch LEFT OUTER JOIN
         app_vat_group_details INNER JOIN
         app_vat_group ON app_vat_group_details.id_vat_group = app_vat_group.id_vat_group INNER JOIN
         app_vat ON app_vat_group_details.id_vat = app_vat.id_vat ON sales_invoice_detail.id_vat_group = app_vat_group.id_vat_group LEFT OUTER JOIN
         items ON sales_invoice_detail.id_item = items.id_item
WHERE (sales_invoice.status = 2) AND (sales_invoice.trans_date &lt;= @StartDate) AND (sales_invoice.trans_date &gt;= @EndDate)
GROUP BY app_branch.name, contacts.name, contacts.gov_code, contacts.code, contacts.telephone, items.code, sales_invoice_detail.item_description, sales_invoice_detail.quantity, sales_invoice_detail.unit_price, sales_invoice_detail.unit_cost, sales_invoice_detail.discount, sales_invoice.number, sales_invoice.trans_date, sales_invoice.comment, 
         sales_rep.name
ORDER BY `Date`</CommandText>
