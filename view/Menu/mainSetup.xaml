<Page x:Class="Cognitivo.Menu.mainSetup"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" Loaded="Page_Loaded"
      d:DesignHeight="400" d:DesignWidth="500"
	  Title="mainSetup">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Frame x:Name="frameContainer" Grid.Row="0"/>
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Stretch">
            <StackPanel.Background>
                <SolidColorBrush Color="Black" Opacity="0.05"/>
            </StackPanel.Background>
            <Button x:Name="btnPrev" Content="Previous" Click="btnPrev_Click" Margin="10" Style="{StaticResource btn_green}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
            <Button x:Name="btnNext" Content="Next" Click="btnNext_Click" Margin="10" Style="{StaticResource btn_green}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        </StackPanel>
    </Grid>
</Page>
