          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SalesOrderSummaryTableAdapter" GeneratorDataComponentClassName="SalesOrderSummaryTableAdapter" Name="SalesOrderSummary" UserDataComponentName="SalesOrderSummaryTableAdapter">
