              <Mapping SourceColumn="id_sales_invoice" DataSetColumn="id_sales_invoice" />
              <Mapping SourceColumn="DEBITO" DataSetColumn="DEBITO" />
              <Mapping SourceColumn="credit" DataSetColumn="credit" />
