<Page x:Name="this" x:Class="Cognitivo.Reporting.ReportViewer"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Cognitivo.Reporting"
      mc:Ignorable="d"
      xmlns:cntrl="clr-namespace:cntrl;assembly=cntrl"
      d:DesignHeight="600" d:DesignWidth="800"
      Title="ReportViewer" Loaded="this_Loaded">
    <Grid>
        <cntrl:ReportPanel ApplicationName="{Binding appName, ElementName=this}" />
    </Grid>
</Page>